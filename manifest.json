{
   "background": {
      "scripts": [ "js/backend.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "AdBan"
   },
   "content_scripts": [
      {
         "js": [ "js/jquery-3.2.1.min.js", "js/baidu/baidu.ad.js" ],
         "matches": [ "*://www.baidu.com/*" ],
         "run_at": "document_start"
      },
      {
         "js": [ "js/jquery-3.2.1.min.js", "js/360/so.ad.js" ],
         "matches": [ "*://www.so.com/*" ],
         "run_at": "document_start"
      }
   ],
   "description": "致力于清理掉一切浏览器广告，百度、360 等。\n 并同时支持隐藏身份和访问提速等功能。",
   "icons": {
      "128": "images/logo.png",
      "16": "images/logo.png",
      "48": "images/logo.png"
   },
   "manifest_version": 2,
   "name": "AdBan",
   "permissions": [ "storage", "cookies", "webRequest", "webRequestBlocking", "*://*.baidu.com/*", "*://*.so.com/*", "tabs" ],
   "version": "0.0.2",
   "web_accessible_resources": [ "images/*" ]
}
